{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Frontend for Blog Application",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "postcss ./src/styles/index.css -o ./dist/styles.css",
    "watch": "npx postcss styles.css -o dist/styles.css --watch",
    "preview": "vite preview",
